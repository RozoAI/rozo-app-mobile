{
  "cli": {
    "version": ">= 16.20.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "prebuildCommand": "npx expo prebuild --clean && bash scripts/fix-android-manifest.sh",
      "env": {
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "prebuildCommand": "npx expo prebuild --clean && bash scripts/fix-android-manifest.sh",
      "android": {
        "resourceClass": "large",
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "prebuildCommand": "npx expo prebuild --clean && bash scripts/fix-android-manifest.sh",
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
